{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 1,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "class-methods-use-this": 0,
    "no-use-before-define": 0
  }
}
